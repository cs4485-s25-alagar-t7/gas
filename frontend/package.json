        "react-scripts": "4.0.3",
        "recharts": "^2.15.1",
        "tailwindcss": "^4.0.12",
        "typescript": "^4.1.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
